{
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "name": "SkateMyRide",
  "scripts": {
    "build": "npm run build --workspaces --if-present",
    "prebuild": "npm run prebuild --workspaces --if-present",
    "pretest": "npm run prebuild --workspaces --if-present",
    "start": "npm start --workspaces --if-present",
    "test": "npm test --workspaces --if-present",
    "dev": "vite",
    "frontend:dev": "cd src/SkateMyRide_frontend && npm run dev",
    "frontend:build": "cd src/SkateMyRide_frontend && npm run build",
    "frontend:serve": "cd src/SkateMyRide_frontend && npm run serve"
  },
  "type": "module",
  "workspaces": [
    "src/SkateMyRide_frontend"
  ],
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fortawesome/fontawesome-free": "^6.5.2",
    "@headlessui/react": "^2.1.0",
    "@heroicons/react": "^2.1.4",
    "@metamask/detect-provider": "^2.0.0",
    "@mui/material": "^5.15.20",
    "@plugnet/wallet": "^0.20.0",
    "@react-google-maps/api": "^2.19.3",
    "all": "^0.0.0",
    "date-fns": "^3.6.0",
    "firebase": "^10.12.2",
    "history": "^5.3.0",
    "postcss": "^8.4.38",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-google-autocomplete": "^2.7.3",
    "react-google-places-autocomplete": "^4.0.1",
    "react-icons": "^5.2.1",
    "react-router-dom": "^6.23.1",
    "tailwindcss": "^3.4.4",
    "web3": "^4.10.0",
    "yarn": "^1.22.22"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.19",
    "vite": "^5.3.1"
  }
}
